{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "types": [
      "node"
    ],
    "jsx": "preserve",
    "incremental": true,
    "typeRoots": [
      "./types",
      "./node_modules/@types",
      "node_modules/@types",
    ],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/testAdmin/tableTest/page.jsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
